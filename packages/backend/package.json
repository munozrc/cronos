    "fastify": "4.27.0",
    "ffmpeg-static": "5.2.0",
    "fluent-ffmpeg": "2.1.3",
    "ytdl-core": "4.11.5"
  },
  "devDependencies": {
    "@eslint/js": "9.5.0",
    "@swc-node/register": "1.9.2",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.5.29",
    "@swc/helpers": "0.5.11",
    "@types/fluent-ffmpeg": "2.1.24",
    "@types/node": "20.14.2",
    "eslint": "9.5.0",
    "eslint-plugin-import": "2.29.1",
    "globals": "^15.6.0",
    "nodemon": "3.1.4",
    "rimraf": "5.0.7",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.13.1"
  }
