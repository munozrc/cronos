{
  "$schema": "https://swc.rs/schema.json",
  "minify": true,
  "jsc": {
    "target": "es6",
    "minify": {
      "compress": true
    },
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "baseUrl": "./src",
    "paths": { "@/*": ["./*"] }
  },
  "module": {
    "type": "commonjs"
  }
}
